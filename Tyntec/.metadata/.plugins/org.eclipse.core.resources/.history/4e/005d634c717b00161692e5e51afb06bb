package org.java.test.tyntec.main.core;

import java.util.Random;

import org.java.test.tyntec.types.EnumTypes;

public class RandomEnum{

	/*
	 * Method to Randomize the values from EnumTypes
	 */
	public static EnumTypes randomTypes(){
		EnumTypes[] EnumTypess = EnumTypes.values();
		Random generator = new Random();
		return EnumTypess[generator.nextInt(EnumTypess.length)];
	}
}